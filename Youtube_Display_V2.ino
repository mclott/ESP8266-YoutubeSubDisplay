  },
};

unsigned short convertColor(char c)
{
  return display.color565((c & 1) * 255, ((c >> 1) & 1) * 255, (c >> 2) * 255);
}

void firstScreen()
{
  int i = 0;
  for (int y = 0; y < 64; y++)
    for (int x = 0; x < 64; x++)
      //display.drawPixel(x, y,0); //
      display.drawPixel(x, y, convertColor(yt[i++]));
      display.setTextColor(myWHITE);
      display.fillRect(0, 53, 64, 64, display.color565(0, 0, 0));
      display.setCursor(0,54);
      display.print("");
}

void updateScreen(float subCount) 
{
  static int scroller = 0;
  scroller++;

  for (int y = 38; y < 52; y++)
   for (int x = 0; x < 64; x++)
       display.drawPixel(x, y, 0);

 int i = 0;
  for (int y = 0; y < 64; y++)
    for (int x = 0; x < 64; x++)
    {
      if(!yt[i] | y > 42)
      {
        int col = x >> 2;
        int row = ((y - scroller) / 6); 
        int rx = x & 3;
        int ry = (y - (scroller % 6) + 6) % 6;
        display.drawPixel(x, y, digits[(col + row) & 1][ry][rx] * 0b00111);
      }
      i++;
    }

  display.setTextColor(0xffff);

  numStr = String((int)subCount);
  
  if (subCount > 99999) {
      numStr = String(subCount/1000,2) + "K";
      }
  if (subCount > 999999) {
      numStr = String(subCount/1000000,2) + "M";
      }    
  if (subCount > 9999999) {
      numStr = String(subCount/1000000,1) + "M";
      }     
  if (subCount > 99999999) {
      numStr = String(subCount/1000000,0) + "M";
      } 
  
  displayText(numStr, 38);
  }

